module data.Boolean

data Bool  : type
data true  : Bool
data false : Bool

trait Boolean for Bool {
    def (||) : Bool -> Bool
    def (&&) : Bool -> Bool
    
}

define Boolean {
    def self(true) (||) _ = self
    def            (||) b = b

    def self(false) (&&) _ = self
    def             (&&) b = b
}

def not : Bool -> Bool
def not true  = false
def not false = true
