module data.try.control

from data.try import *
from control.control import *

define Functor Try {
  def map (Success v) f = Success (f v)
  def map (Failure s) f = Failure s
}

define Applicative Try {
  def (<*>) (Success f) v = v map f
  def (<*>) (Failure s) _ = Failure s
}

define Monad Try {
  def (>>=) (Success v) f = f v
  def (>>=) (Failure s) _ = Failure s
}
