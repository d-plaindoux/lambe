module core.Int

trait Arithmetic A for A {
    def (+) : A -> A
    def (-) : A -> A
    def (*) : A -> A
    def (/) : A -> A
}

define Arithmetic Int {
    def (+) i = native "Int.add" self i
    def (-) i = native "Int.minus" self i
    def (*) i = native "Int.multiply" self i
    def (/) i = native "Int.divide" self i
}

