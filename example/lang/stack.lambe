import error
import list
import option

data Stack a {
    value : List a
}

data StackUnderFlow

impl forall a. Stack a {
    sig new : self
    def new = Stack $ List empty

    sig push : self -> a -> self
    def push a = self with value = a :: self.value

    sig peek : self -> Option a
    def peek = self value

    sig pop : self -> self with Error StackUnderFlow
    def pop = self with value = self value fold { StackUnderFlow raise } { _ tail }
}
