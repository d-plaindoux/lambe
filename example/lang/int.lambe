import char
import algebraic

kind int : type

impl int {
    sig (--) : self -> self
    sig (++) : self -> self
    sig (+)  : self -> self -> self // native
    sig (-)  : self -> self -> self // native
    sig abs  : self -> self         // native
    sig toChar : self -> Char       // native

    def (--) = self - 1
    def (++) = self + 1

    // TODO ...
}

impl SemiGroup int {
    def compose b = self + b
}

impl Monoid int {
    def neutral = 0
}
