impl functor

type Option a =
  data None
| data Some { value : a }

impl for Option a {
    sig fold : self -> (None -> b) -> (Some a -> b) -> b

    def None.fold f _ = f self
    def Some.fold _ f = f self
}

impl Function Option {
    def Some.fmap f = Some $ f value
    def None.fmap _ = None
}
