data Pair a b {
    fst : a
    snd : b
}

impl for Pair a b {
    sig (,) : self -> b -> Pair self b for a
    def (,) b = Pair self b

    sig switch : self -> Pair b a
    def switch = self.snd , self.fst

    sig zip : self -> Pair c d -> Pair (Pair a c) (Pair b d)
    def zip p = (self.fst , p.fst), (self.snd , p.snd)
}
