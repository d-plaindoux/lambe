impl int
impl char
impl algebraic
impl list

data String { value: List char }

impl String {
    sig length : self -> int
    sig chatAt : self -> int -> Option char
    sig (++) : self -> self -> self
    sig replace : self -> char -> char -> self
    sig indexOf : self -> char -> Option int
}

impl SemiGroup String {
    def compose b = String (self.value + b.value)
}

impl Monoid String {
    def neutral = String neutral
}
