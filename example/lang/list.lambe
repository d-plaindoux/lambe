type List a =
  data Nil
| data Cons {
    head : a
    tail : List a
  }

impl for List a {
    sig fold : self -> (Nil -> b) -> (Cons a -> b) -> b
    def Nil.fold f _ = f self
    def Cons.fold _ f = f self

    sig (::) : self -> List a -> List a for a
    def (::) l = Cons self l
}

impl Monad List {
    def pure a = a :: Nil

    def Nil.(<*>) _ = Nil
    def Cons.(<*>) a = self tail <*> $ a map $ self head

    def Nil.flatten = Nil
    def Cons.flatten = self.head // TODO
}
