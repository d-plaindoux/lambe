impl lang.pair
impl lang.list

data Memory { v:int }
data Cursor { v:int }
data Input
data Output
data Loop   { l : Program }
data Nullify
type Token = Memory | Cursor | Input | Output | Loop | Nullify
type Program = List Token

data Pure
data Impure
type Purity = Pure | Impure

type Parsed = Pair Program Purity
